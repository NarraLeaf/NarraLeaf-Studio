{
  "name": "narraleaf-studio",
  "version": "0.0.1",
  "private": true,
  "description": "IDE for NarraLeaf Projects",
  "homepage": "https://github.com/NarraLeaf/NarraLeaf-Studio#readme",
  "bugs": {
    "url": "https://github.com/NarraLeaf/NarraLeaf-Studio/issues"
  },
  "workspaces": [
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NarraLeaf/NarraLeaf-Studio.git"
  },
  "license": "MPL-2.0",
  "author": "NarraLeaf Project",
  "main": "dist/main/index.js",
  "scripts": {
    "build": "yarn build:main && yarn build:apps",
    "build:dev": "yarn build:main:dev && yarn build:apps:dev",
    "build:apps": "node project/build/build-apps.js",
    "build:apps:dev": "node project/build/build-apps.js --dev",
    "build:main": "node project/build/build-main.js",
    "build:main:dev": "node project/build/build-main.js --dev",
    "dev": "node project/app/dev-electron.js",
    "pack-electron": "yarn clean && node project/app/pack-electron.js",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "electron-store": "^9.0.0",
    "lucide-react": "^0.546.0",
    "mime-types": "^3.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/electron-store": "^3.2.2",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.21",
    "chokidar": "^4.0.3",
    "ejs": "^3.1.10",
    "electron": "^38.3.0",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.25.11",
    "esbuild-postcss": "^0.0.4",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3",
    "typescript": "^5.7.3",
    "ws": "^8.17.0"
  },
  "packageManager": "yarn@4.10.3+sha512.c38cafb5c7bb273f3926d04e55e1d8c9dfa7d9c3ea1f36a4868fa028b9e5f72298f0b7f401ad5eb921749eb012eb1c3bb74bf7503df3ee43fd600d14a018266f"
}
